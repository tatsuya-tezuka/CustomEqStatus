                exec "SS-TCRDEM.FRAME_PACKING           ,2                  , ;";
                exec "SS-TCRDEM.SUB_CARR                ,235296.0           , ;";
                exec "SS-TCRDEM.VITERBI_DECODE          ,ON(CCSDS)          , ;";
                exec "SS-TCRDEM.TLM_MOD_INDEX           ,1.10               , ;";
