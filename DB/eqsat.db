                exec "S-ACQ_TRKRX.BIT_RATE              ,64516.0            , ;";   //追加 2017/08/16 as-06 
            break;
            case  500kbps;
                exec "S-ACQ_TRKRX.MOD_FORMAT            ,PCM-QPSK           , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.CARR_LOOP_BAND        ,MEDIUM             , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.AGC_LOOP_BAND         ,MEDIUM             , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.TLM_MOD_INDEX         ,0.0                , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.IF_CENTER_FREQ        ,70000000           , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.DATA_FORMAT           ,NRZ-L              , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.VITERBI_DECODE        ,ON(CCSDS)          , ;";   //追加 2017/08/16 as-06
