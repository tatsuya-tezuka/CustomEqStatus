                exec "S-ACQ_TRKRX.BIT_RATE              ,500000.0           , ;";   //追加 2017/08/16 as-06 
            break;
            case  1Mbps;
                exec "S-ACQ_TRKRX.MOD_FORMAT            ,PCM-QPSK           , ;";   //追加 2017/08/16 as-06
                exec "S-ACQ_TRKRX.CARR_LOOP_BAND        ,MEDIUM             , ;";   //追加 2017/08/16 as-06
