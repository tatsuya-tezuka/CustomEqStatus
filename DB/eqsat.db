            break;
            case  RNGOFF; // RNG MOD OFFの場合
                exec "SS-TCRMOD.MODULATION              , ON  , ;";
            break;
        endofmac;

        ////////////////////////////////////////
        // ＣＭＤ　ＭＯＤ　ＯＦＦ制御用マクロ //
        ////////////////////////////////////////
        mac CTRL.S_CMD_MOD_OFF;
            // パラメータ無し
            case *;
                exec "SS-TCRMOD.MODULATION              , OFF , ;";
            break;
        endofmac;

    endofmacdef;
end;
