            case  9.2Mbps;
                exec "X-HRX.DEMODULATION                ,QPSK               , ;";
                exec "X-HRX.DEMO_DATA_MODE              ,NRZ-L              , ;";
                exec "X-HRX.VITERBI_ON                  ,OFF                , ;";
                exec "X-HRX.DIFF_DECODE                 ,4PHASE             , ;";
                exec "X-HRX.DEMO_DATA_RATE_CONT         ,9200000            , ;";
                exec "X-HRX.MODULATION                  ,QPSK               , ;";
                exec "X-HRX.MOD_DATA_MODE               ,NRZ-L              , ;";
                exec "X-HRX.CONV_SETTING_CONV_ON        ,OFF                , ;";
                exec "X-HRX.DIFF_ENCODE                 ,4PHASE             , ;";
                exec "X-HRX.MOD_DATA_RATE_CONT          ,9200000            , ;";
