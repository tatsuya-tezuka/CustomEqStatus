                exec "SS-TCRDEM.BIT_RATE                ,2000000.0          , ;";
                exec "SS-TCRDEM.FRAME_PACKING           ,19                 , ;";
//              exec "SS-TCRDEM.SUB_CARR                ,                   , ;";
                exec "SS-TCRDEM.VITERBI_DECODE          ,OFF                , ;";
                exec "SS-TCRDEM.TLM_MOD_INDEX           ,0.0                , ;";
                exec "SS-TCRDEM.FILTER_STYLE            ,ROOT_NYQ           , ;";
                exec "SS-TCRDEM.CARR_LOOP               ,MEDIUM             , ;";
                exec "SS-TCRDEM.AGC_LOOP                ,MEDIUM             , ;";
                exec "SS-TCRMOD.MOD_FORMAT_MODE         ,PCM-QPSK           , ;";
                exec "SS-TCRMOD.DATA_MODE               ,NRZ_L              , ;";
                exec "SS-TCRMOD.BIT_RATE                ,2000000.0          , ;";
