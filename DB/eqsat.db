// sat 衛星名称 ***後ろにブランクが必須***
// no  衛星番号
// rot STDN/DSN 受信機モード
// s-coh Ｓ帯コヒーレントレシオ(s240.0/221.0)
// x-coh Ｘ帯コヒーレントレシオ(x880.0/221.0)
// br  ビットレート変更通知有無(Y,N) 臼田のみ
// coh コヒーレント運用有無(Y,N)
// sat 対象衛星管制ホスト名 臼田のみ

//     sat,     no, band, rot,    s-coh,        x-coh,     br, coh,  sat;
satinf GEOTAIL ,23, S/X, STDN, s240.0/221.0, x900.0/221.0, N,   Y, MS-175;

    //----------------------------//
    // ＡＧＣ校正対象受信設備情報 //
    //----------------------------//
    agcrx;
        s-rx-agc Y;          // KSC34 UDSC64
        x-rx-agc Y;          // KSC34
        x-rx1-agc N;         //       UDSC64
        x-rx2-agc N;         //       UDSC64
        sx-acqrx-s-agc Y;    // KSC34
        sx-acqrx-x-agc Y;    // KSC34
        x-qpskrx-agc N;      // KSC34
    endofagcrx;

    //----------------------------//
    // ＲＡＲＲ使用帯域情報       //
    //----------------------------//
    rarrband S/X;

    //----------------------------//
    // ノミナル送信周波数情報[Hz] //
    //----------------------------//
    txfq S=2081000000;

    // ノミナル受信周波数情報[Hz]
    rxfq S=2259909502, X=8474660633;

    //--------------------------//
    // 固有情報ファイル一覧情報 //
    //--------------------------//
    prd  /sos/k3ctrl/data/prd/geotail/;       // 予報値ファイル格納ディレクトリ
