            break;
        endofmac;

        //////////////////////////////////////
        // ＲＮＧ　ＭＯＤ　ＯＮ制御用マクロ //
        //////////////////////////////////////
        mac CTRL.S_RNG_MOD_ON;
            case  CMDON; // CMD MOD ONの場合
