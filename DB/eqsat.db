//  agc ;                                        ＡＧＣ校正データファイル
//  rng ;                                        ＲＮＧ局内ディレイ値ファイル
    plan /sos/k3ctrl/data/plan/exec/geotail/; // 局運用計画ファイル格納ディレクトリ

    //-------------------//
    // 復調系設備 新／旧 //
    //-------------------//
    eq EQU_NEW_OLD=NEW;

    //--------------------------//
    // Ｘ帯送信設備使用有無情報 //
    //--------------------------//
    xtx N;

    //----------------------//
    // Ｘ帯受信設備対象情報 //
    //----------------------//
    xrxsel RX1;  // KSCはRX1固定

    //--------------------------//
    // 周波数ランプ制御許可情報 //
    //--------------------------//
    rampcnt;
