                exec "S-ACQ_TRKRX.BIT_RATE              ,1000000.0          , ;";   //追加 2017/08/16 as-06           
            break;
        endofmac;

        mac CTRL.MOD_INDEX;
            // パラメータ無し
            case CMD;
                exec "SS-TCRMOD.CMD_MOD_INDEX           ,0.4                , ;";
                exec "SS-TCRMOD.RNG_MOD_INDEX           ,0.0                , ;";
            break;
            case RNG;
