       x-tx-ramp     N; // KSC34 UDSC64
       s-rx-ramp     Y; // KSC34 UDSC64
       x-rx-ramp     Y; // KSC34
       x-rx1-ramp    N; //       UDSC64
       x-rx2-ramp    N; //       UDSC64
