                exec "SS-TCRDEM.VITERBI_DECODE          ,OFF                , ;";
                exec "SS-TCRDEM.TLM_MOD_INDEX           ,0.0                , ;";
                exec "SS-TCRDEM.FILTER_STYLE            ,ROOT_NYQ           , ;";
                exec "SS-TCRDEM.CARR_LOOP               ,MEDIUM             , ;";
                exec "SS-TCRDEM.AGC_LOOP                ,MEDIUM             , ;";
                exec "S-TRKRX.MOD_FORMAT                ,PCM-QPSK           , ;";
                exec "S-TRKRX.DATA_MODE                 ,NRZ_L              , ;";
                exec "S-TRKRX.BIT_RATE                  ,2000000.0          , ;";
                exec "S-TRKRX.TLM_MOD_INDEX             ,0.2                , ;";
