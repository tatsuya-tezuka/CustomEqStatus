// <<2013-TURBO>>
// RARRの初期設定項目は、通常/再生モードの共通項目を設定する
// 各モードに必要なものは、パス開始時に設定する。
//
        mac INIT.DSNTCR-RARR;
            // パラメータ無し
            case *;
                exec "DSNTCR.RNG_CONT           ,STOP                  , ;";
                exec "DSNTCR.RNG_MODE           ,REGEN                 , ;";
                exec "DSNTCR.RNG_MOD_INDEX      ,1.3                   , ;";
                exec "DSNTCR.RNG_FREQ_SEL       ,Xfup/2^14             , ;";
                exec "DSNTCR.RNG_INTEG_TIME1    ,2                     , ;";
                exec "DSNTCR.RNG_INTEG_TIME2    ,4                     , ;";
                exec "DSNTCR.RARR_SAMPLERATE    ,1/10                  , ;";
                exec "DSNTCR.RNG_NUM            ,60                    , ;";
                exec "DSNTCR.RNG_SAT_WAIT_TIME  ,15                    , ;";
                exec "DSNTCR.RNG_TEST_DOPP_SEL  ,OFF                   , ;";
                exec "DSNTCR.UP_REF_FREQ        ,7171559414.0          , ;";
//                exec "DSNTCR.DOWN_REF_FREQ      ,8425864198.0          , ;";
                exec "DSNTCR.1/2WAY_MODE        ,2WAY                  , ;";
                exec "DSNTCR.TRP_RATIO          ,COH-880/749           , ;";
                exec "DSNTCR.RNG_PRE            ,ON                    , ;";
           break;
       endofmac;

       mac INITC.DSNTCR-RNG_MOD;
           // パラメータ無し
           case *;
                exec "DSNTCR.RNG_MOD_INDEX      ,1.3                   , ;";
                exec "DSNTCR.TX_MOD_INDEX       ,0.0                   , ;";
           break;
       endofmac;

// <<2013-TURBO>>
        mac INIT.S70-DC;
            // パラメータ無し
            case *;
                exec "S70-DC.LOCAL_ATT_CONT             ,0dB                , ;";
                exec "S70-DC.COH_INCOH                  ,COH                , ;";
                exec "S70-DC.SET_FREQ                   ,7171559414.0       , ;";
            break;
        endofmac;
