                exec "SS-TCRMOD.RNG_MOD_INDEX           ,0.55               , ;";
            break;
            case CMD+RNG;
                exec "SS-TCRMOD.CMD_MOD_INDEX           ,0.4                , ;";
                exec "SS-TCRMOD.RNG_MOD_INDEX           ,0.55               , ;";
            break;
        endofmac;

        //////////////////////////////////////
        // ＲＮＧ　ＭＯＤ　ＯＮ制御用マクロ //
        //////////////////////////////////////
        mac CTRL.S_RNG_MOD_ON;
