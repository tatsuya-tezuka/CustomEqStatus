                exec "DSNTCR.TLM_ID_BIT_LOC     ,0                     , ;";
                exec "DSNTCR.TLM_ID_LENGTH      ,0                     , ;";
                exec "DSNTCR.SUB_SCH_TO_LCK     ,1                     , ;";
                exec "DSNTCR.SUB_LCK_TO_SCH     ,1                     , ;";
                exec "DSNTCR.RX_TB_CRC          ,OFF                   , ;";
                exec "DSNTCR.TB_DERANDOMIZE     ,OFF                   , ;";
                exec "DSNTCR.ECHO_BER           ,STOP                  , ;";
                exec "DSNTCR.TLM_BER            ,STOP                  , ;";
                exec "DSNTCR.FER_CNT            ,STOP                  , ;";
                exec "DSNTCR.RX_CARR_DOPP_PRE   ,OFF                   , ;";
