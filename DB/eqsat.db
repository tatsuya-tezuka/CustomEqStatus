        endofmac;

        mac CTRL.MOD_INDEX;
            // パラメータ無し
            case CMD;
                exec "SS-TCRMOD.CMD_MOD_INDEX           ,0.4                , ;";
                exec "SS-TCRMOD.RNG_MOD_INDEX           ,0.0                , ;";
            break;
            case RNG;
                exec "SS-TCRMOD.CMD_MOD_INDEX           ,0.0                , ;";
